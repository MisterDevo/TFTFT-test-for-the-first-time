{
  "name": "test-for-the-first-time",
  "version": "1.0.0",
  "description": "TFTFT : Test For The First Time",
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir public/cov",
    "start": "node ./bin/www",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R nyan && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage" : "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir test/coverage"
  },
  "engines": {
    "node": "4.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MisterDevo/TFTFT.git"
  },
  "author": "misterdevo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MisterDevo/TFTFT/issues"
  },
  "homepage": "https://github.com/MisterDevo/TFTFT#readme",
  "dependencies": {
    "body-parser": "~1.14.2",
    "cookie-parser": "~1.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "istanbul-middleware": "^0.2.2",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  }
}
